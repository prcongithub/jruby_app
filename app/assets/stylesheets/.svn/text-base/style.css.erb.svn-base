textarea{
	height:100px;
	resize:both;
}

table{
	width:100%;
	text-align:left;
}

tr.head{
	background: #ddd;
	border: solid 1px #ccc;
	border-radius: 5px;
}

th{
	padding:5px;
}

td{
 padding:5px;
}

.cover{
	 position: absolute;
	 top:0;
	 left:0;
	 background-color: blue;
	 z-index:2;
	 opacity:0.4;
	 filter: alpha(opacity = 50);
}

#cover {
	position:absolute;
	background-color:#000;
	opacity:0.4;
}

.cover_container{
	position:relative;
}

tr.table_row{
	background: #eee;
	border: solid 1px #ccc;
	border-radius: 5px;
}

.table_row td{
	
}

.question_list{
	padding: 10px;
	margin:10px;
	background: #DDD;
	border: solid 1px #AAA;
	min-width:100px;
	min-height:300px;
}

.question_box{
	padding: 10px;
	margin:10px;
	background: #DDD;
	border: solid 1px #AAA;
	min-width:540px;
	min-height:300px;
}

.question_list_icon{
	margin: 3px;
	padding: 3px;
	font-weight: normal;
}

.timer_box{
	font-weight: bold;
	color: red;
}

.question_list_icon_selected{
	background: black;
	color: white;
	margin: 3px;
	padding: 3px;
	font-weight: bold;
}


.top_search_box{
	border-radius: 10px;
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	background-color: #eee;
	padding: 8px;
	margin:5px;
  border: 1px solid black;	
}

.notic_div{
	position: absolute; 
	text-align: center; 
	width: 1000px; 
	top: 85px;
}

.header_box{
	border: 1px solid #ddd;
	padding: 10px;
	background: #ccc;
	font-weight: bold;
	font-size: 14px;
	color: #000;
}

a:hover{
	background:none;
	text-decoration:underline;
}

.chat_panel{
	padding:5px;
	color:white;
	position: fixed;
	width:300px;
	bottom: 0;
	right: 0;
	z-index: 9999;
	border-bottom: none;
	background: black;
	margin: 0 1%;
	font-family: "Helvetica Neue",Arial,Helvetica,Geneva,sans-serif;
	line-height: normal;
	text-align:left;
}

.chat_panel_main{
	position: absolute;
	left: 0;
	background: black;
	bottom: 0px;
	display: none;
	width: 99%;
	height:450px;
	border: 1px solid 
	#555;
	overflow: hidden;
	padding-bottom: 2px;
}

.typing_div{
	min-height:20px;
}

#chat_users{
	height:400px;
}

#header_chat_panel{
	padding:5px;
	
}

#footer_chat_panel{
	padding:5px;
	
}

.user_chat_header{
	color:black;
}

.user_chat_box{
	color:black;
}

.notice_top{
	background:#000; 
	border:0px; 
	text-align:center; 
	margin:8px; 
	padding: 3px 10px  3px 10px;  
	font-size:16px; 
	color:#fff; 
	-moz-border-radius: 5px; 
	border-radius: 5px;
}

.parent_category{
	border:solid 1px #000;
}

.parent_category :hover{
	color: #fff;
	border: 0px solid red;
	border-top: 0px solid #CECECE;
	background: url("<%= asset_path('bg-link-hover.gif') %>") repeat-y top left;
	text-decoration: underline;
}

.child_category{
	padding:5px;
	color: #fff;
	border: 0px solid red;
	border-top: 0px solid #CECECE;
	background: url("<%= asset_path('bg-link-hover.gif') %>") repeat-y top left;
}

.child_category :hover{
	text-decoration: underline;
}

.categories{
	padding:5px;
}

.sub_category_ul{
	position: absolute;
	left: 345px;
	z-index: 5000;
	width: 200px;
	border: 1px solid #333;
	margin-top:-29px;
}

